<div id="container">
  <div id="header">
    <h1>Artists</h1>
  </div>

  <div id="navigation">
    <%= link_to "New Artist", new_artist_path %>
  </div>

  <div id="menu"></div>

  <div class="content">
    <% @artists.each do |artist| %>
    <p>
      <%= link_to artist.name, artist_path(artist.id) %>
    </p>
    <% end %>
  </div>

  <div id="clear"></div>

  <div id="footer">
  </div>
</div>
