<%= form_for ([@artist, @song]) do |f| %>

  <ul>
    <% @song.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>


  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :audio_url %>
  <%= f.text_field :audio_url %>



  <%= f.submit (@song.new_record? ? "Create" : "Update") %>

  <%= f.label :artist_id %>
  <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>

<% end %>
<br>
<%= link_to '[Back]', songs_path %>
